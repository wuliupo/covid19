{
  "name": "covid-19",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@antv/data-set": "^0.11.4",
    "@antv/g2": "^4.0.6",
    "@rmwc/button": "^6.0.14",
    "@rmwc/card": "^6.0.14",
    "@rmwc/circular-progress": "^6.0.14",
    "@rmwc/data-table": "^6.0.14",
    "@rmwc/grid": "^6.0.14",
    "@rmwc/top-app-bar": "^6.0.14",
    "@rmwc/typography": "^6.0.14",
    "classnames": "^2.2.6",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "normalize.css": "^8.0.1",
    "react": "^16.13.1",
    "react-countup": "^4.3.3",
    "react-dom": "^16.13.1",
    "react-measure": "^2.3.0",
    "react-router-dom": "^5.1.2",
    "swr": "^0.2.0"
  },
  "scripts": {
    "predeploy": "yarn build",
    "deploy": "gh-pages -d build",
    "start": "react-scripts start",
    "build": "yarn lint && NODE_ENV=production PUBLIC_URL=/covid19 react-scripts build",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx ./src",
    "lintfix": "eslint --fix --ext .js,.jsx,.ts,.tsx ./src"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "airbnb-typescript"
    ],
    "rules": {
      "import/extensions": 0,
      "react/prop-types": 0
    },
    "parserOptions": {
      "project": "./tsconfig.json"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/classnames": "^2.2.10",
    "@types/lodash": "^4.14.149",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "@types/react-measure": "^2.0.6",
    "@types/react-router-dom": "^5.1.4",
    "@typescript-eslint/eslint-plugin": "^2.24.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-typescript": "^7.2.1",
    "eslint-config-react-app": "^5.2.1",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^2.5.0",
    "react-scripts": "3.4.1",
    "typescript": "~3.7.2"
  }
}
